{
  "compilerOptions": {
    "composite": true,
    "module": "commonjs",
    "declaration": true,
    "declarationMap": true,
    "noImplicitAny": false,
    "removeComments": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ES2021",
    "sourceMap": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["node", "jest"]
  }
}
